<p id="notice"><%= notice %></p>

<h1>Welcome Teachers</h1>

<h4>Teachers</h4>
<div class="wrapper">
  <table>
    <thead>
      <tr>
        <th>Email</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @users_teachers.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.teacher.first_name %></td>
          <td><%= user.teacher.last_name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>

<%= link_to 'New Grade', new_grade_path %>
